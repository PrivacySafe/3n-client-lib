{
  "name": "@v1nt1248/3nclient-lib",
  "license": "AGPL-3.0-or-later",
  "author": "v1nt1248",
  "version": "0.1.4",
  "description": "Library for 3NWeb clients",
  "type": "module",
  "files": [
    "dist",
    "src/components/"
  ],
  "module": "./dist/ui-3n-lib.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ui-3n-lib.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
    "copy:dts": "cp ./src/types.d.ts ./dist",
    "build": "vite build",
    "preview": "vite preview",
    "stylelint": "stylelint --config .stylelint.config.cjs 'src/**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --config .stylelint.config.cjs --fix 'src/**/*.{vue,scss}'",
    "linter": "eslint -c .eslintrc.cjs --ext .js,.vue,.ts, src",
    "linter:fix": "eslint -c .eslintrc.cjs --fix --ext .js,.vue,.ts, src",
    "lint": "yarn linter && yarn stylelint",
    "lint:fix": "yarn linter:fix && yarn stylelint:fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.4",
    "@iconify-icons/ic": "1.2.13",
    "@iconify/vue": "4.1.2",
    "dayjs": "1.11.12",
    "lodash": "4.17.21",
    "mitt": "3.0.1",
    "sanitize-html": "2.13.0",
    "tslib": "2.6.3",
    "vuedraggable": "^4.1.0"
  },
  "peerDependencies": {
    "pinia": "2.2.1",
    "vue": "3.4.37"
  },
  "devDependencies": {
    "@types/lodash": "4.17.5",
    "@types/node": "20.14.2",
    "@types/sanitize-html": "2.11.0",
    "@typescript-eslint/eslint-plugin": "8.0.1",
    "@typescript-eslint/parser": "8.0.1",
    "@vitejs/plugin-vue": "5.1.2",
    "@vue/compiler-core": "3.4.37",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/runtime-core": "3.4.37",
    "@vuedx/typescript-plugin-vue": "0.7.6",
    "eslint": "9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-vue": "9.27.0",
    "jackspeak": "4.0.1",
    "path": "0.12.7",
    "pinia": "2.2.1",
    "postcss-html": "1.7.0",
    "prettier": "^3.3.3",
    "sass": "1.77.8",
    "stylelint": "16.8.1",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-scss": "6.5.0",
    "typescript": "5.5.4",
    "vite": "5.4.0",
    "vite-plugin-css-injected-by-js": "3.5.1",
    "vite-plugin-dts": "4.0.2",
    "vite-plugin-vue-devtools": "^7.3.7",
    "vitepress": "1.3.2",
    "vue": "3.4.37",
    "vue-tsc": "2.0.29"
  },
  "packageManager": "pnpm@9.7.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  }
}
